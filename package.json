{
  "name": "co-utils",
  "version": "1.0.2",
  "description": "工具库",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "umd": "./dist/index.umd.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "clean:dist": "rimraf dist",
    "build:types": "npm run clean:dist && tsc -b ./tsconfig.types.json",
    "build": "npm run build:types && rollup -c",
    "test": "node test/test.js",
    "pretest": "npm run build",
    "lint": "eslint \"src/**/*.{js,jsx,vue}\" --fix",
    "parser": "husky install",
    "changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0  -n ./changelog-option.js",
    "version": "conventional-changelog -p custom-config -i CHANGELOG.md -s -n ./changelog-option.js && git add CHANGELOG.md",
    "post:version": "git push --tags"
  },
  "author": "cz",
  "license": "MIT",
  "keywords": [
    "工具库",
    "utils"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "babel-eslint": "^10.1.0",
    "compare-func": "^2.0.0",
    "conventional-changelog": "^3.1.25",
    "conventional-changelog-cli": "^2.2.2",
    "conventional-changelog-custom-config": "^0.3.1",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "^7.16.0",
    "husky": "^8.0.2",
    "lint-staged": "12.4.0",
    "prettier": "2.6.2",
    "prettier-eslint": "15.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "typescript": "^4.6.3",
    "vue": "^3.2.45",
    "yarn": "^1.22.19"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "http://43.138.188.22:4873/"
  },
  "lint-staged": {
    "src/**/*.{ts,js,vue,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  }
}
